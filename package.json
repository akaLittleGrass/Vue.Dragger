{
  "name": "vuedragger",
  "version": "1.0.17",
  "description": "a vue component used to drag and zoom something",
  "main": "lib/VueDragger.umd.min.js",
  "unpkg": "lib/VueDragger.umd.min.js",
  "author": "LittleGrass Wei",
  "license": "MIT",
  "keywords": [
    "vue",
    "draggable"
  ],
  "private": false,
  "scripts": {
